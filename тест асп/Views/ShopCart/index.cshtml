@using тест_асп.ViewModel
@model ShopCartViewModels


@{
    Layout = "_Layout";

}


<div class="container">

    @foreach (var al in Model.shopCart.listShopCarItems)
    {
        <div class="alert-warning mt-3">
            <b>Avto:</b>@al.car.name<br />
            <b>Price:</b>@al.car.price.ToString("C")
        </div>
    }
    <hr />
    <div class="btn btn-danger mt-4" asp-controller="Order">оплатить</div>

</div>